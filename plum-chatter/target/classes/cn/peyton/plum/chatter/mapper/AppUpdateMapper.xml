<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.peyton.plum.chatter.mapper.AppUpdateMapper">
	<resultMap id="BaseResultMap" type="cn.peyton.plum.chatter.pojo.AppUpdate">
		<id column="id" property="id" jdbcType="INTEGER"/>
		<result column="url" property="url" jdbcType="VARCHAR"/>
		<result column="version" property="version" jdbcType="VARCHAR"/>
		<result column="status" property="status" jdbcType="INTEGER"/>
		<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
	</resultMap>

	<sql id="Base_Column_List">
		id,url,version,status,create_time
	</sql>

	<insert id="insert" parameterType="cn.peyton.plum.chatter.pojo.AppUpdate" keyColumn="id" keyProperty="id" useGeneratedKeys="true">
		insert into app_update(url,version,status,create_time)
		values (#{url},#{version},#{status},#{createTime})
	</insert>

	<insert id="insertSelective" parameterType="cn.peyton.plum.chatter.pojo.AppUpdate" keyColumn="id" keyProperty="id" useGeneratedKeys="true">
		insert into app_update
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="url != null">
				url,
			</if>
			<if test="version != null">
				version,
			</if>
			<if test="status != null">
				status,
			</if>
			<if test="createTime != null">
				create_time,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="url != null">
				#{url},
			</if>
			<if test="version != null">
				#{version},
			</if>
			<if test="status != null">
				#{status},
			</if>
			<if test="createTime != null">
				#{createTime},
			</if>
		</trim>
	</insert>

	<delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
		delete from app_update
		where id = #{id}
	</delete>

	<update id="updateByPrimaryKeySelective" parameterType="cn.peyton.plum.chatter.pojo.AppUpdate">
		update app_update
		<set>
			<if test="url != null">
				url = #{url},
			</if>
			<if test="version != null">
				version = #{version},
			</if>
			<if test="status != null">
				status = #{status},
			</if>
			<if test="createTime != null">
				create_time = #{createTime},
			</if>
		</set>
		where id = #{id}
	</update>

	<update id="updateByPrimaryKey" parameterType="cn.peyton.plum.chatter.pojo.AppUpdate">
		update app_update
		set 
			url = #{url},
			version = #{version},
			status = #{status},
			create_time = #{createTime}
		where id = #{id}
	</update>

	<select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		select
		<include refid="Base_Column_List"/>
		from app_update
		where id = #{id}
	</select>


	<!-- new create method   -->

</mapper>
